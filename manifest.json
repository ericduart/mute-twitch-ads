{
  "manifest_version": 3,
  "name": "Mute Twitch ads",
  "description": "",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts" : [{
    "matches" : ["https://www.twitch.tv/*"],
    "js" : ["muteTwitchAds.js"]
  }
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
